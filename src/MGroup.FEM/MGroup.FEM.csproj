<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>George Stavroulakis, Dimitris Tsapetis</Authors>
    <Product>MSolve</Product>
    <Description>A series of classes for spatially discretizing computational mechanics problems using the finite element method</Description>
    <PackageProjectUrl>http://mgroup.ntua.gr</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mgroupntua/MSolve.FEM</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MGroup.LinearAlgebra" Version="1.0.251" />
    <PackageReference Include="MGroup.Materials" Version="1.0.298" />
    <PackageReference Include="MGroup.MSolve.Discretization" Version="1.0.294" />
    <PackageReference Include="MGroup.MSolve.Geometry" Version="1.0.294" />
  </ItemGroup>

</Project>
